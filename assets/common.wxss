.progress {
  height: 70rpx;
  padding: 70rpx 60rpx 0;
  margin-bottom: 130rpx;
}

.progress_item {
  width: 100%;
  height: 3px;
  background-color: #cfcfcf;
  position: relative;
}

.progress_item_add {
  height: 3px;
  background-color: #207ce9;
  position: absolute;
  top: 0rpx;
  z-index: 99;
}

.progress_icon {
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}

.progress_icon view {
  flex: 1;
  text-align: center;
  margin-top: -12px;
  color: #fff;
}

.progress_icon view:nth-child(1):before {
  content: '';
}

.progress_icon view:nth-child(2):before {
  content: '';
}

.progress_icon view:nth-child(3):before {
  content: '';
}

.progress_icon view:before {
  display: inline-block;
  width: 20px;
  height: 20px;
  line-height: 20px;
  text-align: center;
  border-radius: 100%;
}

.progress_icon view:nth-child(1):after {
  content: '人脸识别';
}

.progress_icon view:nth-child(2):after {
  content: '身份信息';
}

.progress_icon view:nth-child(3):after {
  content: '访问信息';
}

.progress_icon view:after {
  display: block;
  padding-top: 5px;
  color: #979797;
}

