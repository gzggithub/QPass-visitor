.gray_line {
  width: 100%;
  height: 20rpx;
  background-color: #f5f7fa;
}

.flex {
  display: flex;
}

.border_radius {
  border-radius: 5px;
}

.touch_item {
  width: 100%;
  height: 148rpx;
  font-size: 14px;
  display: flex;
  align-items: center;
  background-color: #fff;
  overflow: hidden;
}

.content {
  width: 100%;
  line-height: 22px;
  margin-right: 0;
  padding-left: 40rpx;
  box-sizing: content-box;
}

.content_company {
  font-size: 14px;
  color: #4a4a4a;
}

.content_time {
  font-size: 12px;
  color: #979797;
}

.del {
  background-color: orangered;
  width: 74px;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  font-size: 14px;
  color: #fff;
  -webkit-transform: translateX(74px);
  transform: translateX(74px);
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
}

.touch-move-active .content, .touch-move-active .del {
  -webkit-transform: translateX(0);
  transform: translateX(0);
}

/* remove scroll-view scroll-bar */

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}


.create_apply {
  width: 128rpx;
  height: 128rpx;
  text-align: center;
  background-color: #FFFFFF;
  font-size: 16px;
  color: #979797;
  border-radius: 100%;
  position: fixed;
  bottom: 10rpx;
  left: 50%;
  transform: translateX(-50%);
  z-index: 999999999999999999999999999 !important;
  /* box-shadow: #3242a0ff 0 0 6px; */
}

.create_apply_inner {
  width: 100rpx;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  margin: 14rpx auto;
  border-radius: 100%; 
  background: linear-gradient(to bottom,#73c0ff, #0675f9);
  font-size: 26px;
  color: #fff;
}

.create_apply_inner_circul {
  width: 48rpx;
  height: 48rpx;
  line-height: 48rpx;
  text-align: center;
  margin: 10rpx auto 0;
  background: linear-gradient(to bottom, #78ceff 0%, #6cc4ff 1%, #207ce9 100%);
  border-radius: 100%;
  color: #2b2b2b;
}

.common_form {
  width: 100%;
  background-color: #FFFFFF;
}

.detail_item {
  height: 116rpx;
  margin: 0 20px;
  display: flex;
  justify-content: center;
  align-items: center;
  border-bottom: 1px solid #e0e0e0;
}

.detail_item_left {
  width: 190rpx;
  font-size: 16px;
  color: #4a4a4a;
}

.detail_item_right {
  flex: 1;
  text-align: right;
  font-size: 16px;
  color: #4a4a4a;
}

.fixed_height {
  height: 20px;
}

.apply_btn.finish_opacity {
  opacity: 0.3;
}

button[disabled].apply_btn {
  background-color: #207ce9;
  color: #fff;
}

/*  加载更多   */
.weui-loading {
  margin: 0 5px;
  width: 20px;
  height: 20px;
  display: inline-block;
  vertical-align: middle;
  -webkit-animation: weuiLoading 1s steps(12, end) infinite;
  animation: weuiLoading 1s steps(12, end) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}
.weui-loadmore {
  width: 65%;
  margin: 1.5em auto;
  line-height: 1.6em;
  font-size: 14px;
  text-align: center;
}
.weui-loadmore__tips {
  display: inline-block;
  vertical-align: middle;
}