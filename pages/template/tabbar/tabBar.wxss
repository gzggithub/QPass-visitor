/**index.wxss**/

.home_container_header {
  background-color: #fff;
}

.canvas-box {
  width: 1px;
  height: 1px;
  overflow: hidden;
  position: fixed;
  top: 0;
  left: -2px;
}

.canvas-box canvas {
  width: 200px;
  height: 200px;
}

.home_container_header_welcome {
  padding-left: 20px;
  padding-bottom: 10px;
  font-size: 18px;
  color: #2e2e39;
}

.home_container_header_date {
  padding-left: 20px;
  font-size: 14px;
  color: #7c86a2;
}

.home_menu_history {
  width: 20px;
  height: 20px;
  position: fixed;
  top: 7px;
  left: 20px;
  z-index: 9;
}

.home_menu_history.home_menu_down {
  width: 18px;
  height: 9px;
  top: 18px;
  left: 35%;
}

.white_fixed {
  width: 100%;
  height: 80px;
  position: fixed;
  top: 0;
  background-color: #FFFFFF;
  z-index: 9;
}

/* 日历 */

.calendar {
  background-color: #fff;
  /* margin-top: 100px !important; */
}

.calendar-header {
  width: 50%;
  font-size: large;
  color: #2e2e39;
  /* padding-bottom: 17px; */
  /* padding-right: 20px; */
  position: fixed;
  top: 0;
  left: 50%;
  transform: translateX(-50%);
  z-index: 9999999999;
}

.calendar-board {
  color: #7c86a2;
  font-size: 14px;
}

.calendar--calendar-weeks {
  position: fixed;
  width: 100%;
  top: 40px;
  margin-bottom: 8px;
  z-index: 9999999999;
}

.calendar--calendar-cell {
  width: 35px !important;
  height: 35px !important;
}

