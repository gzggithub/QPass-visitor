.home_container_main {
  width: 100%;
  padding-left: 10px;
}

.overflow_height{
  /* height: 293px;  */
  height: 680rpx;
  overflow: scroll;
}

.home_container_main_message_title {
  padding-top: 10px;
  padding-bottom: 10px;
  font-size: 14px;
  color: #509702;
}

.home_swiper_box {
  height: 74px;
}

.home_swiper_item {
  box-sizing: border-box;
  width: 288px !important;
  padding-right: 20rpx !important;
}

.home_swiper_box_first {
  box-sizing: border-box;
  height: 74px;
  padding-top: 16px;
  padding-left: 16px;
  padding-bottom: 15px;
  border-radius: 5px;
  background-color: #fff;
}

.home-swiper_box_company {
  height: 20px;
  line-height: 20px;
  margin-bottom: 8px;
  font-size: 14px;
  color: #4a4a4a;
}

.home-swiper_box_time {
  height: 17px;
  line-height: 17px;
  font-size: 12px;
  color: #979797;
}

.home_container_main_schedule_title {
  padding-top: 50rpx;
  padding-bottom: 22rpx;
  padding-left: 26rpx;
  display: flex;
}

.today_schedule{
  flex: 2;
  font-size: 14px;
  color: #7c86a2;
}

.home_more {
  flex: 1;
  text-align: right;
  padding-right: 20px;
  font-size: 12px;
  color: #bdbcc7;
}

.home_container_main_more {
  padding: 10px 0 20px;
  text-align: center;
  font-size: 12px;
  color: #7c86a2;
}
